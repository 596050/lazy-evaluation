{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", { "ignoreComments": true }],
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "comma-dangle": 0
  }
}
